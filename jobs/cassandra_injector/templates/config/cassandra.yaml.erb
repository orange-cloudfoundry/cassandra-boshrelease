cluster_name: <%=properties.cassandra_injector.cluster_name%>
num_tokens: <%=properties.cassandra_injector.num_tokens%>
hinted_handoff_enabled: <%=properties.cassandra_injector.hinted_handoff_enabled%>
max_hint_window_in_ms: <%=properties.cassandra_injector.max_hint_window_in_ms%>
hinted_handoff_throttle_in_kb: <%=properties.cassandra_injector.hinted_handoff_throttle_in_kb%>
max_hints_delivery_threads: <%=properties.cassandra_injector.max_hints_delivery_threads%>
authenticator: <%=properties.cassandra_injector.authenticator%>
authorizer: <%=properties.cassandra_injector.authorizer%>
permissions_validity_in_ms: <%=properties.cassandra_injector.permissions_validity_in_ms%>
partitioner: <%=properties.cassandra_injector.partitioner%>
data_file_directories:
    - <%=properties.cassandra_injector.persistent_directory%>/<%=properties.cassandra_injector.cluster_name%>/data
hints_directory: <%=properties.cassandra_injector.persistent_directory%>/<%=properties.cassandra_injector.cluster_name%>/hint
commitlog_directory: <%=properties.cassandra_injector.persistent_directory%>/<%=properties.cassandra_injector.cluster_name%>/commitlog
disk_failure_policy: <%=properties.cassandra_injector.disk_failure_policy%>
key_cache_size_in_mb: <%=properties.cassandra_injector.key_cache_size_in_mb%>
key_cache_save_period:  <%=properties.cassandra_injector.key_cache_save_period%>
row_cache_size_in_mb: <%=properties.cassandra_injector.row_cache_size_in_mb%>
row_cache_save_period: <%=properties.cassandra_injector.row_cache_save_period%>
saved_caches_directory: <%=properties.cassandra_injector.persistent_directory%>/<%=properties.cassandra_injector.cluster_name%>/saved_caches
commitlog_sync: <%=properties.cassandra_injector.commitlog_sync%>
commitlog_sync_period_in_ms:  <%=properties.cassandra_injector.commitlog_sync_period_in_ms%>
commitlog_segment_size_in_mb:  <%=properties.cassandra_injector.commitlog_segment_size_in_mb%>
seed_provider:
    - class_name: org.apache.cassandra.locator.SimpleSeedProvider
      parameters:
          - seeds: <% seeds = link('seeds') %><% seeds.instances.each do |instance| %><%= instance.address %>,<% end %>
concurrent_reads: <%=properties.cassandra_injector.concurrent_reads%>
concurrent_writes: <%=properties.cassandra_injector.concurrent_writes%>
file_cache_size_in_mb: <%=properties.cassandra_injector.file_cache_size_in_mb%>
memtable_flush_writers: <%=properties.cassandra_injector.memtable_flush_writers%>
trickle_fsync: <%=properties.cassandra_injector.trickle_fsync%>
trickle_fsync_interval_in_kb: <%=properties.cassandra_injector.trickle_fsync_interval_in_kb%>
storage_port: <%=properties.cassandra_injector.storage_port%>
ssl_storage_port: <%=properties.cassandra_injector.ssl_storage_port%>
listen_address: <%= spec.ip %>
start_native_transport: <%=properties.cassandra_injector.start_native_transport%>
native_transport_port: <%=properties.cassandra_injector.native_transport_port%>
native_transport_max_threads: <%=properties.cassandra_injector.native_transport_max_threads%>
native_transport_max_frame_size_in_mb: <%=properties.cassandra_injector.native_transport_max_frame_size_in_mb%>
start_rpc: <%=properties.cassandra_injector.start_rpc%>
rpc_address: <%=spec.id%>
rpc_port: <%=properties.cassandra_injector.rpc_port%>
rpc_keepalive: <%=properties.cassandra_injector.rpc_keepalive%>
rpc_server_type: <%=properties.cassandra_injector.rpc_server_type%>
rpc_min_threads: <%=properties.cassandra_injector.rpc_min_threads%>
rpc_max_threads: <%=properties.cassandra_injector.rpc_max_threads%>
thrift_framed_transport_size_in_mb: <%=properties.cassandra_injector.thrift_framed_transport_size_in_mb%>
incremental_backups: <%=properties.cassandra_injector.incremental_backups%>
snapshot_before_compaction: <%=properties.cassandra_injector.snapshot_before_compaction%>
auto_snapshot: <%=properties.cassandra_injector.auto_snapshot%>
tombstone_warn_threshold: <%=properties.cassandra_injector.tombstone_warn_threshold%>
tombstone_failure_threshold: <%=properties.cassandra_injector.tombstone_failure_threshold%>
column_index_size_in_kb: <%=properties.cassandra_injector.column_index_size_in_kb%>
concurrent_compactors: <%=properties.cassandra_injector.concurrent_compactors%>
read_request_timeout_in_ms: <%=properties.cassandra_injector.read_request_timeout_in_ms%>
range_request_timeout_in_ms: <%=properties.cassandra_injector.range_request_timeout_in_ms%>
write_request_timeout_in_ms: <%=properties.cassandra_injector.write_request_timeout_in_ms%>
cas_contention_timeout_in_ms: <%=properties.cassandra_injector.cas_contention_timeout_in_ms%>
truncate_request_timeout_in_ms: <%=properties.cassandra_injector.truncate_request_timeout_in_ms%>
request_timeout_in_ms: <%=properties.cassandra_injector.request_timeout_in_ms%>
cross_node_timeout: <%=properties.cassandra_injector.cross_node_timeout%>
phi_convict_threshold: <%=properties.cassandra_injector.phi_convict_threshold%>
endpoint_snitch: <%=properties.cassandra_injector.endpoint_snitch%>
dynamic_snitch_update_interval_in_ms: <%=properties.cassandra_injector.dynamic_snitch_update_interval_in_ms%>
dynamic_snitch_reset_interval_in_ms: <%=properties.cassandra_injector.dynamic_snitch_reset_interval_in_ms%>
dynamic_snitch_badness_threshold: <%=properties.cassandra_injector.dynamic_snitch_badness_threshold%>
request_scheduler: <%=properties.cassandra_injector.request_scheduler%>
internode_compression: <%=properties.cassandra_injector.internode_compression%>
inter_dc_tcp_nodelay: <%=properties.cassandra_injector.inter_dc_tcp_nodelay%>
server_encryption_options:
 internode_encryption: <%=properties.cassandra_injector.server_encryption_options.internode_encryption%>
 keystore: /var/vcap/jobs/cassandra_injector/config/certs/<%=spec.ip%>_cassandra.keystore
 keystore_password: <%=properties.cassandra_injector.cass_KSP%>
 truststore: /var/vcap/jobs/cassandra_injector/config/certs/<%=spec.ip%>_cassandra.truststore
 truststore_password: <%=properties.cassandra_injector.cass_KSP%>
 protocol: TLS
client_encryption_options:
 enabled: <%=properties.cassandra_injector.client_encryption_options.enabled%>
##   optional: true
 keystore: /var/vcap/jobs/cassandra_injector/config/certs/<%=spec.ip%>_cassandra.keystore
 keystore_password: <%=properties.cassandra_injector.cass_KSP%>
 truststore: /var/vcap/jobs/cassandra_injector/config/certs/<%=spec.ip%>_cassandra.keystore
 truststore_password: <%=properties.cassandra_injector.cass_KSP%>
 require_client_auth: <%=properties.cassandra_injector.client_encryption_options.require_client_auth%>
 protocol: TLS
