storage_port: <%=properties.cassandra_seed.storage_port%>
ssl_storage_port: <%=properties.cassandra_seed.ssl_storage_port%>
listen_address: <%=spec.ip%>
start_native_transport: <%=properties.cassandra_seed.start_native_transport%>
