truncate_request_timeout_in_ms: <%=properties.cassandra_seed.truncate_request_timeout_in_ms%>
request_timeout_in_ms: <%=properties.cassandra_seed.request_timeout_in_ms%>
cross_node_timeout: <%=properties.cassandra_seed.cross_node_timeout%>
phi_convict_threshold: <%=properties.cassandra_seed.phi_convict_threshold%>
endpoint_snitch: <%=properties.cassandra_seed.endpoint_snitch%>
dynamic_snitch_update_interval_in_ms: <%=properties.cassandra_seed.dynamic_snitch_update_interval_in_ms%>
