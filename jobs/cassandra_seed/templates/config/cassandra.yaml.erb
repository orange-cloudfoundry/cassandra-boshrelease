start_rpc: <%=properties.cassandra_seed.start_rpc%>
rpc_address: <%=spec.ip%>
rpc_port: <%=properties.cassandra_seed.rpc_port%>
rpc_keepalive: <%=properties.cassandra_seed.rpc_keepalive%>
