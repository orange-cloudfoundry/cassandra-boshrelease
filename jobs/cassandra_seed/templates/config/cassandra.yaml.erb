incremental_backups: <%=properties.cassandra_seed.incremental_backups%>
snapshot_before_compaction: <%=properties.cassandra_seed.snapshot_before_compaction%>
auto_snapshot: <%=properties.cassandra_seed.auto_snapshot%>
tombstone_warn_threshold: <%=properties.cassandra_seed.tombstone_warn_threshold%>
tombstone_failure_threshold: <%=properties.cassandra_seed.tombstone_failure_threshold%>
column_index_size_in_kb: <%=properties.cassandra_seed.column_index_size_in_kb%>
concurrent_compactors: <%=properties.cassandra_seed.concurrent_compactors%>
read_request_timeout_in_ms: <%=properties.cassandra_seed.read_request_timeout_in_ms%>
range_request_timeout_in_ms: <%=properties.cassandra_seed.range_request_timeout_in_ms%>
