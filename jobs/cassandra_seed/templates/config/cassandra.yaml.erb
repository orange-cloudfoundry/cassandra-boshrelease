cluster_name: <%=properties.cassandra_seed.cluster_name%>
num_tokens: <%=properties.cassandra_seed.num_tokens%>
hinted_handoff_enabled: <%=properties.cassandra_seed.hinted_handoff_enabled%>
max_hint_window_in_ms: <%=properties.cassandra_seed.max_hint_window_in_ms%>
hinted_handoff_throttle_in_kb: <%=properties.cassandra_seed.hinted_handoff_throttle_in_kb%>
max_hints_delivery_threads: <%=properties.cassandra_seed.max_hints_delivery_threads%>
authenticator: <%=properties.cassandra_seed.authenticator%>
authorizer: <%=properties.cassandra_seed.authorizer%>
permissions_validity_in_ms: <%=properties.cassandra_seed.permissions_validity_in_ms%>
partitioner: <%=properties.cassandra_seed.partitioner%>
data_file_directories:
    - <%=properties.cassandra_seed.persistent_directory%>/<%=properties.cassandra_seed.cluster_name%>/data
hints_directory: <%=properties.cassandra_seed.persistent_directory%>/<%=properties.cassandra_seed.cluster_name%>/hint
commitlog_directory: <%=properties.cassandra_seed.persistent_directory%>/<%=properties.cassandra_seed.cluster_name%>/commitlog
disk_failure_policy: <%=properties.cassandra_seed.disk_failure_policy%>
key_cache_size_in_mb: <%=properties.cassandra_seed.key_cache_size_in_mb%>
key_cache_save_period:  <%=properties.cassandra_seed.key_cache_save_period%>
row_cache_size_in_mb: <%=properties.cassandra_seed.row_cache_size_in_mb%>
row_cache_save_period: <%=properties.cassandra_seed.row_cache_save_period%>
saved_caches_directory: <%=properties.cassandra_seed.persistent_directory%>/<%=properties.cassandra_seed.cluster_name%>/saved_caches
commitlog_sync: <%=properties.cassandra_seed.commitlog_sync%>
commitlog_sync_period_in_ms:  <%=properties.cassandra_seed.commitlog_sync_period_in_ms%>
commitlog_segment_size_in_mb:  <%=properties.cassandra_seed.commitlog_segment_size_in_mb%>
seed_provider:
    - class_name: org.apache.cassandra.locator.SimpleSeedProvider
      parameters:
          - seeds: <% seeds = link('seeds') %><% seeds.instances.each do |instance| %><%= instance.address %>,<% end %>
concurrent_reads: <%=properties.cassandra_seed.concurrent_reads%>
concurrent_writes: <%=properties.cassandra_seed.concurrent_writes%>
file_cache_size_in_mb: <%=properties.cassandra_seed.file_cache_size_in_mb%>
