cluster_name: <%=properties.cassandra_server.cluster_name%>
num_tokens: <%=properties.cassandra_server.num_tokens%>
hinted_handoff_enabled: <%=properties.cassandra_server.hinted_handoff_enabled%>
max_hint_window_in_ms: <%=properties.cassandra_server.max_hint_window_in_ms%>
hinted_handoff_throttle_in_kb: <%=properties.cassandra_server.hinted_handoff_throttle_in_kb%>
max_hints_delivery_threads: <%=properties.cassandra_server.max_hints_delivery_threads%>
authenticator: <%=properties.cassandra_server.authenticator%>
authorizer: <%=properties.cassandra_server.authorizer%>
permissions_validity_in_ms: <%=properties.cassandra_server.permissions_validity_in_ms%>
partitioner: <%=properties.cassandra_server.partitioner%>
data_file_directories:
    - <%=properties.cassandra_server.persistent_directory%>/<%=properties.cassandra_server.cluster_name%>/data
hints_directory: <%=properties.cassandra_server.persistent_directory%>/<%=properties.cassandra_server.cluster_name%>/hint
commitlog_directory: <%=properties.cassandra_server.persistent_directory%>/<%=properties.cassandra_server.cluster_name%>/commitlog
disk_failure_policy: <%=properties.cassandra_server.disk_failure_policy%>
key_cache_size_in_mb: <%=properties.cassandra_server.key_cache_size_in_mb%>
key_cache_save_period:  <%=properties.cassandra_server.key_cache_save_period%>
row_cache_size_in_mb: <%=properties.cassandra_server.row_cache_size_in_mb%>
row_cache_save_period: <%=properties.cassandra_server.row_cache_save_period%>
saved_caches_directory: <%=properties.cassandra_server.persistent_directory%>/<%=properties.cassandra_server.cluster_name%>/saved_caches
commitlog_sync: <%=properties.cassandra_server.commitlog_sync%>
commitlog_sync_period_in_ms:  <%=properties.cassandra_server.commitlog_sync_period_in_ms%>
commitlog_segment_size_in_mb:  <%=properties.cassandra_server.commitlog_segment_size_in_mb%>
seed_provider:
    - class_name: org.apache.cassandra.locator.SimpleSeedProvider
      parameters:
          - seeds: <% seeds = link('seeds') %><% seeds.instances.each do |instance| %><%= instance.address %>,<% end %>
concurrent_reads: <%=properties.cassandra_server.concurrent_reads%>
concurrent_writes: <%=properties.cassandra_server.concurrent_writes%>
file_cache_size_in_mb: <%=properties.cassandra_server.file_cache_size_in_mb%>
memtable_flush_writers: <%=properties.cassandra_server.memtable_flush_writers%>
