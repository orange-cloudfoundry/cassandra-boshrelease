truncate_request_timeout_in_ms: <%=properties.cassandra_server.truncate_request_timeout_in_ms%>
request_timeout_in_ms: <%=properties.cassandra_server.request_timeout_in_ms%>
cross_node_timeout: <%=properties.cassandra_server.cross_node_timeout%>
phi_convict_threshold: <%=properties.cassandra_server.phi_convict_threshold%>
endpoint_snitch: <%=properties.cassandra_server.endpoint_snitch%>
dynamic_snitch_update_interval_in_ms: <%=properties.cassandra_server.dynamic_snitch_update_interval_in_ms%>
