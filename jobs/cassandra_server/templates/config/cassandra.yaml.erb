snapshot_before_compaction: <%=properties.cassandra_server.snapshot_before_compaction%>
auto_snapshot: <%=properties.cassandra_server.auto_snapshot%>
tombstone_warn_threshold: <%=properties.cassandra_server.tombstone_warn_threshold%>
tombstone_failure_threshold: <%=properties.cassandra_server.tombstone_failure_threshold%>
column_index_size_in_kb: <%=properties.cassandra_server.column_index_size_in_kb%>
concurrent_compactors: <%=properties.cassandra_server.concurrent_compactors%>
read_request_timeout_in_ms: <%=properties.cassandra_server.read_request_timeout_in_ms%>
