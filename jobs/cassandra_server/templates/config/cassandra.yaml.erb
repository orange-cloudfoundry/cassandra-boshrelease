native_transport_max_threads: <%=properties.cassandra_server.native_transport_max_threads%>
native_transport_max_frame_size_in_mb: <%=properties.cassandra_server.native_transport_max_frame_size_in_mb%>
start_rpc: <%=properties.cassandra_server.start_rpc%>
rpc_address: <%=spec.ip%>
rpc_port: <%=properties.cassandra_server.rpc_port%>
rpc_keepalive: <%=properties.cassandra_server.rpc_keepalive%>
